<%= form_with model: @user do |form| %>
  <div>
    <%= form.label "Enter your username" %><br>
    <%= form.text_field :signin %><br>
    <% @user.errors.full_messages_for(:signin).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
