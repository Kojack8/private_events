<div id = 'navbar'>
  
  <ul>
    <li class = 'navbar_left'>Event Manager</li>
    <% if logged_in? %>    
      <li>You are Logged In, <%= current_user.name %></li>
      <li><%= link_to 'All Events', events_path, method: :get%></li>
      <li><%= link_to 'New Event', new_event_path, method: :get %></li>
      <li><%= link_to 'Log Out', logout_path, method: :delete %></li>
    <% else %>
      <li><%= link_to 'You are not Logged In', login_path, method: :get %></li>
    <%end%>
  </ul>
</div>